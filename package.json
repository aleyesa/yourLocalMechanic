{
  "name": "yourlocalmechanic",
  "version": "1.0.0",
  "description": "If you have any issues with your automobile, we willhave the solutions. We are a company with expert mechanics stationed in your local area.",
  "main": "server.js",
  "scripts": {
    "start": "npm run build && node dist/server",
    "test": "mocha ./server/test --require @babel/register",
    "build": "babel server/dev -d dist",
